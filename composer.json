{
    "name": "siriusphp/stack_runner",
    "description": "Library that",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "callables",
        "events",
        "middleware",
        "pipes",
        "pipelines"
    ],
    "authors": [
        {
            "name": "Adrian Miu",
            "email": "adrian@sirius.ro"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "psr/container": "^2.0",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Sirius\\StackRunner\\": "src/"
        }
    },
    "provide": {
        "psr/event-dispatcher-implementation": "1.0"
    },
    "scripts": {
        "cs": [
            "php phpcs.phar --standard=PSR2 ./src"
        ],
        "md": [
            "php phpmd.phar ./src xml phpmd.xml"
        ],
        "cbf": [
            "php phpcbf.phar ./src --standard=PSR2 -w"
        ],
        "csfix": [
            "php php-cs-fixer.phar fix ./src --rules=@PSR12"
        ],
        "test": [
            "php vendor/bin/phpunit -c tests/phpunit.xml"
        ]
    }
}
