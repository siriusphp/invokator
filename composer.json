{
    "name": "siriusphp/stack_runner",
    "description": "Library that",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "callables",
        "events",
        "middleware",
        "pipes",
        "pipelines"
    ],
    "authors": [
        {
            "name": "Adrian Miu",
            "email": "adrian@sirius.ro"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "psr/container": "^2.0",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Sirius\\StackRunner\\": "src/"
        }
    },
    "provide": {
        "psr/event-dispatcher-implementation": "1.0"
    },
    "scripts": {
        "stan": [
            "php vendor/bin/phpstan analyse"
        ],
        "csfix": [
            "tools/php-cs-fixer/vendor/bin/php-cs-fixer fix  --standard=PSR-2 src"
        ],
        "test": [
            "php vendor/bin/phpunit -c tests/phpunit.xml"
        ]
    }
}
